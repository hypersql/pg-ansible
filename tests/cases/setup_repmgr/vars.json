{
  "use_system_user": false,
  "pg_owner": "opensql",
  "pg_group": "opensql",
  "use_hostname": false,
  "pg_extension_list": ["sslutils"],
  "pg_data": "/var/lib/pgsql/14/main/data",
  "pg_wal": "/opt/pgwal",
  "repmgr_failover": "automatic",
  "repmgr_reconnect_attempts": 2,
  "repmgr_reconnect_interval": 2
}
